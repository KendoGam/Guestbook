<% include header %>
<% if(entries.length){ %>
    <% entries.forEach((entry)=>{%>
    <div class="panel panel-default">
        <div class="panel-heading">
            <div class="text-muted pull-right">
                <%= entry.created %>
            </div>
            <%= entry.title %> /*imprimir informacion*/
        </div>
      <div class="panel-body">
          <%= entry.body %>
      </div>  
    </div>
<%}) %>
<% }else{ %>  /*consultas*/
    <p>No hay visitas todav√≠a!<a href="/new-entry">Agregar una!</a></p>
<%}%>

<%include footer%>